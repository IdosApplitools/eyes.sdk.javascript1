{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,

    "target": "ES2017",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "downlevelIteration": true,
    "stripInternal": true,
    "experimentalDecorators": false
  },
}