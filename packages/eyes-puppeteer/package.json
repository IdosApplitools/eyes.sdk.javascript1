{
  "name": "@applitools/eyes-puppeteer",
  "version": "1.2.2",
  "description": "Applitools Eyes SDK for Puppeteer",
  "keywords": [
    "eyes-puppeteer",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "puppeteer"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.5.7",
    "@applitools/visual-grid-client": "15.1.1"
  },
  "peerDependencies": {
    "puppeteer": ">=5.3.0"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "^2.0.1",
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "^0.4.2",
    "@applitools/sdk-shared": "^0.4.1",
    "eslint": "^7.9.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.0.1",
    "prettier": "1.19.0",
    "puppeteer": "5.3.1",
    "spec-xunit-file": "0.0.1-3"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "render": "node ../sdk-shared/scripts/render.js",
    "test": "yarn test:it",
    "test:it": "yarn run:docker yarn mocha --exit --no-timeouts ./test/it/*.spec.js",
    "test:coverage": "yarn run:docker yarn mocha --exit --config .mocharc.cvg.js",
    "coverage:generate": "coverage-tests generate ../sdk-shared/coverage-tests/generic/index.js --name 'eyes-puppeteer'",
    "coverage:report": "coverage-tests report ../sdk-shared/coverage-tests/generic/index.js --name 'eyes-puppeteer'",
    "coverage:sandbox": "yarn coverage:generate && yarn test:coverage && yarn coverage:report --sandbox",
    "coverage:prod": "yarn coverage:generate && yarn test:coverage && yarn coverage:report",
    "preversion": "bongo preversion && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion",
    "deps": "bongo deps",
    "build": "docker build -t applitools/puppeteer-chrome .",
    "run:docker": "docker run --env APPLITOOLS_API_KEY=$APPLITOOLS_API_KEY_SDK --env APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-puppeteer' --env APPLITOOLS_BATCH_ID=$(uuidgen) --env XUNIT_FILE=coverage-test-report.xml -it --init --rm -v $(pwd):/sandbox applitools/puppeteer-chrome",
    "moca": "yarn run:docker yarn mocha --no-timeouts"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
