{
  "name": "@applitools/eyes.webdriverio",
  "version": "2.30.1",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "repository": "https://github.com/applitools/eyes.sdk.javascript1",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Applitools Team <team@applitools.com> (http://www.applitools.com/)",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts' '**/*.js'",
    "render": "node ../sdk-shared/scripts/render.js",
    "link": "node ../sdk-shared/scripts/link.js",
    "build": "yarn build:dist && yarn build:types",
    "build:dist": "tsc",
    "build:types": "api-extractor ./src/index.ts --out ./types/index.d.ts --external-globals WebdriverIO",
    "generate:tests": "coverage-tests generate node_modules/@applitools/sdk-shared/coverage-tests/generic/index.js --name 'eyes.webdriverio.javascript4'",
    "test": "yarn build:dist && yarn test:it && yarn test:coverage",
    "test:it": "mocha --no-timeouts \"test/it/**/*.js\"",
    "test:coverage": "yarn generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-webdriverio-4' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "report": "coverage-tests report node_modules/@applitools/sdk-shared/coverage-tests/generic/index.js --name 'eyes.webdriverio.javascript4'",
    "setup": "yarn docker:setup",
    "docker:setup": "node ../sdk-shared/src/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "deps": "bongo deps",
    "gh:test": "gh workflow run test.yml -f sdks='wdio-legacy' -f links='eyes-sdk-core eyes-api utils visual-grid-client sdk-shared' -f linking-depth='1'",
    "gh:publish": "gh workflow run publish.yml -f sdks='wdio-legacy'",
    "preversion": "bongo preversion && yarn build",
    "version": "bongo version",
    "postversion": "bongo postversion"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/eyes-api": "1.0.1",
    "@applitools/eyes-sdk-core": "12.19.2",
    "@applitools/utils": "1.1.3",
    "@applitools/visual-grid-client": "15.8.5"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.1.1",
    "@applitools/sdk-coverage-tests": "2.3.11",
    "@applitools/sdk-fake-eyes-server": "2.1.1",
    "@applitools/sdk-release-kit": "^0.13.0",
    "@applitools/sdk-shared": "0.8.10",
    "@types/webdriverio": "4.13.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^85.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "geckodriver": "^1.19.1",
    "husky": "^4.3.7",
    "mocha": "^8.0.1",
    "prettier": "^2.1.2",
    "shared-examples-for": "^0.1.3",
    "spec-xunit-file": "0.0.1-3",
    "typescript": "^3.9.6",
    "webdriverio": "~4.14.4"
  },
  "peerDependencies": {
    "webdriverio": "~4.14.4"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
