{
  "name": "@applitools/spec-driver-selenium",
  "version": "1.3.0",
  "keywords": [
    "selenium",
    "selenium-webdriver",
    "webdriver",
    "webdriverjs",
    "applitools",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "packages/spec-driver-selenium"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "ttsc",
    "test": "mocha ./test/**/*.spec.ts --no-timeouts -r ts-node/register -r @applitools/test-utils/mocha-hooks/docker",
    "setup": "yarn docker:setup",
    "docker:setup": "node ../scripts/scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "deps": "bongo deps",
    "gh:test": "gh workflow run test.yml --ref $(git rev-parse --abbrev-ref HEAD) -f packages='spec-selenium spec-selenium@3' -f links='types utils test-utils'",
    "gh:publish": "gh workflow run publish.yml --ref $(git rev-parse --abbrev-ref HEAD) -f packages='spec-selenium'",
    "postinstall": "if [ \"$APPLITOOLS_INSTALL_FRAMEWORK\" = true ] && [ ! -z $APPLITOOLS_SELENIUM_MAJOR_VERSION ]; then packagejson=`cat package.json`; yarn add --ignore-scripts --no-lockfile selenium-webdriver@$APPLITOOLS_SELENIUM_MAJOR_VERSION; echo \"$packagejson\" > package.json; fi",
    "preversion": "bongo preversion",
    "version": "bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/types": "1.0.20",
    "@applitools/utils": "1.2.4"
  },
  "devDependencies": {
    "@applitools/api-extractor": "^1.2.3",
    "@applitools/scripts": "1.0.1",
    "@applitools/sdk-release-kit": "0.13.4",
    "@applitools/test-utils": "1.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@types/selenium-webdriver": "^4.0.15",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "selenium-webdriver": "^4.0.0",
    "ts-node": "^10.2.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.5.1-rc"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  }
}
