{
  "name": "@applitools/selenium-spec",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@applitools/utils": "^1.2.3"
  },
  "devDependencies": {
    "@applitools/api-extractor": "^1.1.3",
    "@applitools/scripts": "^1.0.1",
    "@applitools/test-utils": "^1.0.7",
    "@types/node": "^16.10.2",
    "@types/selenium-webdriver": "^4.0.15",
    "mocha": "^9.1.2",
    "selenium-webdriver": "^4.0.0-rc-2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "yarn build:dist",
    "build:dist": "tsc",
    "test": "yarn build:dist && yarn test:e2e",
    "test:e2e": "mocha --no-timeouts './test/e2e/*.spec.js'",
    "setup": "yarn docker:setup",
    "docker:setup": "node ../scripts/scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "deps": "bongo deps"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts"
