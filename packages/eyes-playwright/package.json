{
  "name": "@applitools/eyes-playwright",
  "version": "1.5.1",
  "description": "Applitools Eyes SDK for Playwright",
  "keywords": [
    "eyes-playwright",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "playwright"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@applitools/eyes-api": "0.0.2",
    "@applitools/eyes-sdk-core": "12.14.2",
    "@applitools/utils": "1.1.0",
    "@applitools/visual-grid-client": "15.5.14"
  },
  "peerDependencies": {
    "playwright": ">=1.0.0"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.0.0",
    "@applitools/sdk-coverage-tests": "2.3.5",
    "@applitools/sdk-fake-eyes-server": "2.1.1",
    "@applitools/sdk-release-kit": "0.12.1",
    "@applitools/sdk-shared": "0.8.5",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "mocha": "^8.0.1",
    "playwright": "1.8.0",
    "prettier": "^2.1.2",
    "spec-xunit-file": "0.0.1-3",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "lint": "eslint '**/*.ts' '**/*.js'",
    "render": "node ../sdk-shared/scripts/render.js",
    "bind": "node ../sdk-shared/scripts/link.js",
    "build": "yarn build:dist && yarn build:types",
    "build:dist": "tsc",
    "build:types": "api-extractor ./index.ts --out ./index.d.ts --external-modules playwright",
    "test": "yarn build:dist && yarn test:it",
    "test:it": "mocha --no-timeouts ./test/it/*.spec.js",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-playwright' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "coverage:generate": "coverage-tests generate node_modules/@applitools/sdk-shared/coverage-tests/generic/index.js --name 'eyes-playwright'",
    "coverage:report": "coverage-tests report node_modules/@applitools/sdk-shared/coverage-tests/generic/index.js --name 'eyes-playwright'",
    "coverage:sandbox": "yarn coverage:generate && yarn test:coverage && yarn coverage:report --sandbox",
    "coverage:prod": "yarn coverage:generate && yarn test:coverage && yarn coverage:report",
    "preversion": "bongo preversion && yarn build && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion",
    "deps": "bongo deps"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  }
}
