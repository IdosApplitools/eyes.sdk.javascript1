    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.13.8",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
