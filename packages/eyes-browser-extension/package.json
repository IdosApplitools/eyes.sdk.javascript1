    "path-browserify": "^1.0.1",
    "playwright": "^1.12.2",
    "process": "^0.11.10",
    "selenium-webdriver": "^4.0.0-beta.4",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.3",
    "webextension-polyfill": "^0.7.0",
